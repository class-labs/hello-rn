{
  "name": "mw-portal",
  "version": "0.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm run watchman:clean && expo start",
    "watchman:clean": "command -v watchman >/dev/null && watchman watch-del \"$PWD\" >/dev/null || true",
    "clean": "npm run watchman:clean && rm -rf ./.expo ./dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@intl/t": "^1.1.0",
    "@tamagui/config": "^1.92.1",
    "expo": "~50.0.14",
    "expo-constants": "~15.4.5",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "tamagui": "^1.92.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/babel__core": "^7.20.5",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functions": "^0.0.7",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-t": "^1.7.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "prettier": {
    "singleQuote": true,
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "^react$",
      "^react-native$",
      "<THIRD_PARTY_MODULES>",
      "",
      "^[./]"
    ]
  },
  "private": true
}
