{
  "name": "hello-rn",
  "version": "0.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run clean:watchman && expo start",
    "clean:watchman": "command -v watchman >/dev/null && watchman watch-del \"$PWD\" >/dev/null || true",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run typecheck && npm run format:check"
  },
  "dependencies": {
    "expo": "~50.0.17",
    "expo-constants": "~15.4.6",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.10",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "private": true
}
